<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mm.mydorm">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.LoginActivity">
        </activity>
        <activity android:name=".activity.Guide">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.StudentInfoNo"></activity>
        <activity android:name=".activity.StudentInfoYes"></activity>
        <activity android:name=".activity.SelectHow" ></activity>
        <activity android:name=".activity.ChooseDormForOne"></activity>
        <activity android:name=".activity.ChooseDormForTwo"></activity>
        <activity android:name=".activity.ChooseDormForTri"></activity>
        <activity android:name=".activity.ChooseDormForMore"></activity>
        <activity android:name=".activity.SuccessNotify"></activity>
    </application>

</manifest>